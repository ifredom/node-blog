doctype
html(class="no-js")
  head
    block meta
      - description = blog.description
      - author = blog.author

      meta(charset="utf-8")
      meta(ame="description", content=description)
      meta(ame="author", content=author)
      meta(name="viewport", content="width=device-width, initial-scale=1")
      meta(name="keywords" content="ifredom, nodejs, node, express, javascript, vue, react, 前端, socket.io")

      //- Set render engine for 360 browser
      meta(name="renderer" content="webkit")
      //- No Baidu Siteapp
      meta(http-equiv="Cache-Control" content="no-siteapp")
      link(rel="icon" type="image/png" href="favicon.ico")

      title  后台管理系统

    block scripts
      link(rel="stylesheet", href="/lib/bootstrap/bootstrap.min.css")
      link(rel="stylesheet", href="/font/iconfont.css")
      link(rel="stylesheet", href="/adminasset/css/app.css")

      script(type="text/javascript", src="/lib/jquery/jquery.2.2.0.min.js")
      script(type="text/javascript", src="/lib/bootstrap/bootstrap.min.js")
  body
    block content
    block footer

