{
    "restartable": "rs",
    "verbose": false,
    "ignore": [".git", ".svn", ".vscode", "node_modules"],
    "env": {
        "NODE_ENV": "development"
    },
    "ext": "js json jade",
    "execMap": {
        "": "node",
        "js": "node --harmony",
        "py": "python",
        "rb": "ruby"
    }
}
